{
    "clickables": {

        "parameterized": {
            "Actions Order Info": {    
                "xpath": "//a[normalize-space(text())=\"ACTION-BUTTON\"]",
                "placeholder": "ACTION-BUTTON"
            }
        }
    },

    "waitables": {

        "uniques": {
            "Iteractable Section": {
                "findBy": "class name",
